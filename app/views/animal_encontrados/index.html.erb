<p id="notice"><%= notice %></p>

<h1>Animal Encontrados</h1>

<table>
  <thead>
    <tr>
      <th>Specie</th>
      <th>Sex</th>
      <th>Founddate</th>
      <th>Foundplace</th>
      <th>Contact</th>
      <th>Ps</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @animal_encontrados.each do |animal_encontrado| %>
      <tr>
        <td><%= animal_encontrado.specie %></td>
        <td><%= animal_encontrado.sex %></td>
        <td><%= animal_encontrado.founddate %></td>
        <td><%= animal_encontrado.foundplace %></td>
        <td><%= animal_encontrado.contact %></td>
        <td><%= animal_encontrado.ps %></td>
        <td><%= link_to 'Show', animal_encontrado %></td>
        <td><%= link_to 'Edit', edit_animal_encontrado_path(animal_encontrado) %></td>
        <td><%= link_to 'Destroy', animal_encontrado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Animal Encontrado', new_animal_encontrado_path %>
