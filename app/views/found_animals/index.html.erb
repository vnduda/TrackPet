<p id="notice"><%= notice %></p>

<h1>Found Animals</h1>

<table>
  <thead>
    <tr>
      <th>Species</th>
      <th>Sex</th>
      <th>Founddate</th>
      <th>Foundplace</th>
      <th>Contact</th>
      <th>Ps</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @found_animals.each do |found_animal| %>
      <tr>
        <td><%= found_animal.species %></td>
        <td><%= found_animal.sex %></td>
        <td><%= found_animal.founddate %></td>
        <td><%= found_animal.foundplace %></td>
        <td><%= found_animal.contact %></td>
        <td><%= found_animal.ps %></td>
        <td><%= found_animal.User_id %></td>
        <td><%= link_to 'Show', found_animal %></td>
        <td><%= link_to 'Edit', edit_found_animal_path(found_animal) %></td>
        <td><%= link_to 'Destroy', found_animal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Found Animal', new_found_animal_path %>
