<p id="notice"><%= notice %></p>

<h1>Animal Losts</h1>

<table>
  <thead>
    <tr>
      <th>Specie</th>
      <th>Sex</th>
      <th>Lostdate</th>
      <th>Lostplace</th>
      <th>Contact</th>
      <th>Ps</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @animal_losts.each do |animal_lost| %>
      <tr>
        <td><%= animal_lost.specie %></td>
        <td><%= animal_lost.sex %></td>
        <td><%= animal_lost.lostdate %></td>
        <td><%= animal_lost.lostplace %></td>
        <td><%= animal_lost.contact %></td>
        <td><%= animal_lost.ps %></td>
        <td><%= link_to 'Show', animal_lost %></td>
        <td><%= link_to 'Edit', edit_animal_lost_path(animal_lost) %></td>
        <td><%= link_to 'Destroy', animal_lost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Animal Lost', new_animal_lost_path %>
